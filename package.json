{
  "name": "svelte-app",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "start": "vite preview",
    "lint": "eslint src",
    "asbuild:untouched": "asc assembly/index.ts --target debug --exportRuntime",
    "asbuild:optimized": "asc assembly/index.ts --target release -Ospeed --exportRuntime",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "test": "node tests",
    "gh-publish": "npm run copy-html:gh && npm run build && sleep 5 && node ./gh-pages.js && sleep 5 && npm run copy-html:dev",
    "copy-html:gh": "MODE_GH=1 node copyHtml.js",
    "copy-html:dev": "MODE_GH=0 node copyHtml.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "assemblyscript": "^0.27.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^3.2.3",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "svelte": "^3.55.1",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.6",
    "vite": "^4.1.1"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.27.0",
    "replace-in-file": "^6.3.5"
  }
}
