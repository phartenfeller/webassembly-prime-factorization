{
  "name": "svelte-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "lint": "eslint src",
    "asbuild:untouched": "asc assembly/index.ts --target debug --exportRuntime",
    "asbuild:optimized": "asc assembly/index.ts --target release --exportRuntime",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "test": "node tests",
    "gh-publish": "npm run copy-html:gh && npm run build && node ./gh-pages.js && npm run copy-html:dev",
    "copy-html:gh": "BASE_PATH=/webassembly-prime-factorization node copyHtml.js",
    "copy-html:dev": "node copyHtml.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "assemblyscript": "^0.19.18",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "gh-pages": "^3.2.3",
    "postcss": "^8.3.11",
    "postcss-load-config": "^3.1.0",
    "rollup": "^2.59.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.44.1",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^2.2.19"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.19.17",
    "replace-in-file": "^6.3.2",
    "sirv-cli": "^1.0.14"
  }
}
